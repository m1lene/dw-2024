<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>loginServelet</servlet-name>
        <servlet-class>Control.loginServelet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletExemplo1</servlet-name>
        <servlet-class>Control.ServletExemplo1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginServelet</servlet-name>
        <url-pattern>/loginServelet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletExemplo1</servlet-name>
        <url-pattern>/ServletExemplo1</url-pattern>
    </servlet-mapping>
</web-app>
